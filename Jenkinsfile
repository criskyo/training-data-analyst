
Pipeline{
  stages{
    stage('inicializacion'){
      steps{
        echo 'inicializando'
      }
    }
    stage('pruebas unitarias'){
      steps{
        echo 'realizando pruebas'
      }
    }
    stage('construccion'){
      steps{
        echo 'construyendo'
      }
    }
    stage('publicacion'){
      steps{
        echo 'publicando'
      }
    }
  }
}
    
